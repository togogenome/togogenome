DEFINE sql:select-option "order"

SELECT DISTINCT ?meo_id ?name
FROM <http://togogenome.org/graph/meo/>
WHERE {
  VALUES ?meo_uri { <%= meos %>}
  BIND(?meo_uri AS ?meo_id)
  ?top_meo_id  rdfs:subClassOf  <http://www.w3.org/2002/07/owl#Thing>.
  ?meo_id rdfs:subClassOf ?top_meo_id  OPTION (TRANSITIVE, T_DIRECTION 1, T_MIN(0), T_STEP("step_no") AS ?step) .
  ?top_meo_id rdfs:label ?name .
}
