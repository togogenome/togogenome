DEFINE sql:select-option "order"

<%= prefix[:tgstat] %>

SELECT DISTINCT ?taxonomy_id ?project_num ?gene_num ?rrna_num ?trna_num ?ncrna_num
FROM <%= ontology[:stats] %>
WHERE {
  VALUES ?taxonomy_id { <%= taxonomies %>}
  ?taxonomy_id tgstat:bioproject ?project_num ;
               tgstat:gene ?gene_num ;
               tgstat:rrna ?rrna_num ;
               tgstat:trna ?trna_num ;
               tgstat:ncrna ?ncrna_num .
}
