DEFINE sql:select-option "order"

SELECT DISTINCT ?mpo_id ?name
WHERE {
  VALUES ?mpo_uri { <%= mpos %>}
  BIND(?mpo_uri AS ?mpo_id)
  GRAPH <%= ontology[:mpo] %> {
    ?mpo_id a ?type ; #dummy
            rdfs:subClassOf* ?top_mpo_id .
    ?top_mpo_id rdfs:subClassOf  <http://purl.jp/bio/01/mpo#MPO_00000> ;
                rdfs:label ?name .
  }
}
